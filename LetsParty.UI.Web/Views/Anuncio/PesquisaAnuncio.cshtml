@model LetsParty.Domain.ViewModel.AnuncioViewModel
@{
    ViewBag.Title = "Resultado Pesquisa";
}

<!DOCTYPE html>
<html lang="pt-br">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=iso-8859-1">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <title>Shop Homepage - Start Bootstrap Template</title>
    <!-- Bootstrap Core CSS -->

    <link href="/Template/css/bootstrap.css" rel="stylesheet" />
    <!-- Custom CSS -->
    <link href="~/Template/css/CarouselRows.css" rel="stylesheet" />
    <link href="/Template/css/shop-homepage.css" rel="stylesheet">
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>
<body>
    <!-- Navigation -->
    @Html.Partial("_MenuBusca")
    <!-- Page Content -->
    <div class="container">
        <div class="row">
            @Html.Partial("_MenuLateral")
            <div class="col-md-9">
                <hr>
                @if (Model != null)
                {
                    foreach (var anuncios in Model.ListaViewModel)
                    {
                        { Html.RenderPartial("PartialPesquisaAnuncio", anuncios); }
                    }
                }
            </div>
        </div>
    </div>
    <!-- /.container -->
    <div class="container">
        <hr>
        <!-- Footer -->
        <footer>
            <div class="row">
                <div class="col-lg-12">
                    <p>Copyright &copy; Festeje.com 2015</p>
                </div>
            </div>
        </footer>
    </div>
    <!-- /.container -->
    <!-- jQuery -->
    <script src="js/jquery.js"></script>
    <!-- Bootstrap Core JavaScript -->
    <script src="js/bootstrap.min.js"></script>
</body>
</html>
