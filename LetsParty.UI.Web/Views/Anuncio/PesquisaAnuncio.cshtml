@model LetsParty.Domain.ViewModel.AnuncioViewModel
@{
    ViewBag.Title = "Resultado Pesquisa";
}

<!DOCTYPE html>
<html lang="pt-br">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=iso-8859-1">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <title>Shop Homepage - Start Bootstrap Template</title>
    <!-- Bootstrap Core CSS -->

    <link href="/Template/css/bootstrap.css" rel="stylesheet" />
    <!-- Custom CSS -->
    <link href="~/Template/css/CarouselRows.css" rel="stylesheet" />
    <link href="/Template/css/shop-homepage.css" rel="stylesheet">
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>
<body>
    <!-- Navigation -->
   @Html.Partial("_MenuBusca")
    <!-- Page Content -->
    <div class="container">
        <div class="row">
          @Html.Partial("_MenuLateral")
            <div class="col-md-9">
                <hr>
                @if (Model != null)
                {
                    foreach (var anuncios in Model.ListaViewModel)
                    {
                        { Html.RenderPartial("PartialPesquisaAnuncio", anuncios); }
                    }
                }
                <div class="row carousel-row">
                    <div class="col-xs-8 col-xs-offset-2 slide-row">
                        <div id="carousel-3" class="carousel slide slide-carousel" data-ride="carousel">
                            <!-- Indicators -->
                            <ol class="carousel-indicators">
                                <li data-target="#carousel-3" data-slide-to="0" class="active"></li>
                            </ol>

                            <!-- Wrapper for slides -->
                            <div class="carousel-inner">
                                <div class="item active">
                                    <img src="http://lorempixel.com/150/150?rand=7" alt="Image">
                                </div>
                                <div class="item">
                                    <img src="http://lorempixel.com/150/150?rand=8" alt="Image">
                                </div>
                                <div class="item">
                                    <img src="http://lorempixel.com/150/150?rand=9" alt="Image">
                                </div>
                            </div>
                        </div>
                        <div class="slide-content">
                            <h4>Example product</h4>
                            <p>
                                Lorem ipsum dolor sit amet, consetetur sadipscing elitr,
                                sed diam nonumy eirmod tempor invidunt ut labore et dolore magna aliquyam erat,
                            </p>
                        </div>
                        <div class="slide-footer">
                            <span class="pull-right buttons">
                                <button class="btn btn-sm btn-default"><i class="fa fa-fw fa-eye"></i> Show</button>
                                <button class="btn btn-sm btn-primary"><i class="fa fa-fw fa-shopping-cart"></i> Buy</button>
                            </span>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- /.container -->
    <div class="container">
        <hr>
        <!-- Footer -->
        <footer>
            <div class="row">
                <div class="col-lg-12">
                    <p>Copyright &copy; Festeje.com 2015</p>
                </div>
            </div>
        </footer>
    </div>
    <!-- /.container -->
    <!-- jQuery -->
    <script src="js/jquery.js"></script>
    <!-- Bootstrap Core JavaScript -->
    <script src="js/bootstrap.min.js"></script>
</body>
</html>
